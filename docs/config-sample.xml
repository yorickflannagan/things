<?xml version="1.0" encoding="UTF-8"?>
<config>
	<!-- bootstrap -->
	<jvm>
		<!-- flags na munheca: -server -Xbatch -->
		<minMemory>512</minMemory>
		<maxMemory>2048</maxMemory>
		<vmflags>xpto</vmflags>
		<classpath>
			<path>qualquerlugar</path>
			<file>qualquer arquivo</file>
		</classpath>
		<properties>
			<property name="java.lib.path" value="qualquerlugar" />
		</properties>
	</jvm>

	<!-- Runner -->
	<jndi implementation="org.crypthing.things.appservice.ServicesFactory" />
	<worker>
		<class>br.gov.caixa.mqx.test.BasicBatch</class>
		<threads>1</threads>
		<restartable>false</restartable>
		<sleep>2000</sleep>
	</worker>

	<sandbox>
		<property name="max-commit" value="20" />
		<property name="max-put" value="100" />
	</sandbox>

	<snmp>
		<property name="org.crypthing.things.SNMPTrap" value="org.crypthing.things.SNMPTrap" />
		<property name="org.crypthing.things.batch.udpAddress" value="127.0.0.1/8163" />
		<property name="org.crypthing.things.batch.rootOID" value="0.1.4320" />
		<property name="org.crypthing.things.batch.retry" value="2" />
		<property name="org.crypthing.things.batch.timeout" value="5000" />
	</snmp>

	<!-- java:jdbc/ -->
	<datasources>
		<!-- java:jdbc/oracns -->
		<jdbc	name="oracns"
				driver="oracle.jdbc.OracleDriver"
				url="jdbc:oracle:oci:@10.192.230.89:1530:ORADES01"
				validationQuery="select * from dual">
			<property name="password" value="zxc1vbn"/>
			<property name="user-name" value="usr_cns"/>
		</jdbc>
	</datasources>

	<!-- java:mqx -->
	<mqxconnectors>
		<!-- java:mqx/mqdes -->
		<mqxconnector name="mqdes" driver="br.gov.caixa.mqx.MQXConnection">
			<context>
				<property name="address" value="10.192.224.110(1414)" />
				<property name="channel" value="RJNT011B.TO.RJD1" />
				<property name="manager" value="RJD1" />
			</context>
			<queues>
				<queue name="getqueue">
					<property name="object" value="LQ.REQ.ICE.TR" />
					<property name="input" value="true" />
					<property name="convert" value="false" />
					<property name="convertMsgId" value="false" />
					<property name="convertCorrelId" value="false" />
					<property name="msgMaxLen" value="1024" />
				</queue>
				<queue name="putqueue">
					<property name="object" value="LQ.RSP.CNS.AUDIT" />
					<property name="input" value="false" />
					<property name="convert" value="false" />
					<property name="convertMsgId" value="false" />
					<property name="convertCorrelId" value="false" />
				</queue>
				<queue name="putgetqueue">
					<property name="object" value="LQ.REQ.ICE.TR" />
					<property name="input" value="false" />
					<property name="convert" value="false" />
					<property name="convertMsgId" value="false" />
					<property name="convertCorrelId" value="false" />
				</queue>
				<queue name="getputqueue">
					<property name="object" value="LQ.RSP.CNS.AUDIT" />
					<property name="input" value="true" />
					<property name="convert" value="false" />
					<property name="convertMsgId" value="false" />
					<property name="convertCorrelId" value="false" />
					<property name="msgMaxLen" value="1024" />
				</queue>
			</queues>
		</mqxconnector>
	</mqxconnectors>
</config>